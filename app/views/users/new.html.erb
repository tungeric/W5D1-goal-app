<h1>New user</h1>
<p>Find me in app/views/users/new.html.erb</p>

<form action="<%=users_url%>" method="post">
  <input type="hidden" name="authenticity_token" value="<%=form_authenticity_token%>">
  <label>Username:
  <input type="text" id="username" name="user[username]" value="<%=@user.username%>"></label>
  <label>Password:
  <input type="password" id="password" name="user[password]" value=""></label>
  <input type="submit" name="" value="Create User">
</form>
